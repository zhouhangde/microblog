<script src="/js/jquery-2.2.4.min.js"></script>
<script type="text/javascript">
	$("#login").on("click",".tab",function(){
		$(this).siblings().removeClass("active");
		$(this).addClass("active");
		$("#forgetPasswordTab").css({display:'none'});
		if($(this).text() == "登录"){
			$("#submit").html("登录");
			$("#loginTab").css({display:'block'});
			$("#registTab").css({display:'none'});
		}else{
			$("#submit").html("立即注册");
			$("#registTab").css({display:'block'});
			$("#loginTab").css({display:'none'});
		}
	});
	$("#design").on("click",".designLi",function(){
		$(this).siblings().removeClass("active");
		$(this).addClass("active");
	});
	$("#submit").click(function(){
		var telephone = $("#telephone").val();
		var password = $("#password").val();
		location.href = "index.ejs?login=true"
	});
	$("#forgetPassword").click(function(){
		$(".headTab").html("<div style='text-align:center;font-size:28px;color:#BA9E43;'>忘记密码<div>");
		$("#loginTab").css({display:'none'});
		$("#forgetPasswordTab").css({display:'block'});
		$("#submit").html("完成");
	});
	$(".eye").click(function(){
		if($(this).parent().find("#password").attr("type") == "password"){
			$(this).find("img").attr("src","/img/browse.png");
			$(this).parent().find("#password").attr("type","text");
		}else{
			$(this).find("img").attr("src","/img/eye.png");
			$(this).parent().find("#password").attr("type","password");
		}
	});
	
	$(".aHover").hover(function(){
		$(".userDiv").css({display: 'block'});
	},function(){
		$(".userDiv").css({display: 'none'});
	});
</script>
</html>